<p id="notice"><%= notice %></p>

<h1>Listing sub categories</h1>
<%= link_to "Add Sub Cat",new_admin_category_sub_category_path(params[:category_id]) %>
<table>
	<thead>
		<tr>
			<th>Sub Cat</th>
			<th>Cat</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @sub_categories.each do |sub_category| %>
		<tr>
			<td><%= sub_category.name %></td>
			<td><%= sub_category.category.name %></td>
			<td><%= link_to "delete",admin_category_sub_category_path(params[:category_id], sub_category.id), method: :delete %></td>
			<td><%= link_to "sub sub cat", admin_category_sub_category_sub_sub_categories_path(params[:category_id],sub_category) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
			
